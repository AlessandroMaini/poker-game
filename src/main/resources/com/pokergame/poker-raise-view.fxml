<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<DialogPane prefHeight="121.0" prefWidth="367.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pokergame.PokerRaiseController">
   <buttonTypes>
      <ButtonType fx:constant="OK" />
      <ButtonType fx:constant="CANCEL" />
   </buttonTypes>
   <content>
      <Slider fx:id="sliderRaise" showTickLabels="true" showTickMarks="true" snapToTicks="true" />
   </content>
   <header>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Amount to add:">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
   </header>
</DialogPane>
